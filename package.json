{
  "name": "@pagopa/io-functions-assets",
  "description": "A set of fnctions used to serve IOpublic assets",
  "author": "IO team",
  "repository": "https://github.com/pagoPA/io-functions-assets",
  "version": "1.4.2",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install --javascript",
    "start:host": "func start --javascript",
    "start": "npm-run-all --parallel start:host watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "tslint -p .",
    "deploy": "npm run build && func azure functionapp publish agid-functions-app-test",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "predeploy": "npm-run-all build dist:modules"
  },
  "devDependencies": {
    "@azure/functions": "^1.2.0",
    "@types/azure-sb": "^0.0.37",
    "@types/documentdb": "^1.10.5",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.15",
    "@types/lolex": "^3.1.1",
    "auto-changelog": "^2.2.1",
    "danger": "^4.0.2",
    "danger-plugin-digitalcitizenship": "^0.3.1",
    "fast-check": "^1.16.0",
    "italia-tslint-rules": "^1.1.3",
    "italia-utils": "^4.1.0",
    "jest": "^24.8.0",
    "jest-mock-express": "^0.1.1",
    "lolex": "^5.1.1",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@pagopa/ts-commons": "^9.2.0",
    "abort-controller": "^3.0.0",
    "applicationinsights": "^1.7.4",
    "azure-sb": "^0.11.1",
    "azure-storage": "^2.10.3",
    "documentdb": "^1.12.2",
    "durable-functions": "^1.4.3",
    "express": "^4.15.3",
    "fp-ts": "1.17.4",
    "io-functions-commons": "^14.6.0",
    "io-functions-express": "^0.1.0",
    "io-ts": "1.8.5"
  },
  "resolutions": {
    "fp-ts": "1.17.4"
  }
}
